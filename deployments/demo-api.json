{
  "requestPolicies": {
    "rateLimiting": {
      "rateInRequestsPerSecond": 10,
      "rateKey": "TOTAL"
    }
  },
  "routes": [
    {
      "backend": {
        "body": "{\"version\" :\"1.0\"}",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          }
        ],
        "status": 200,
        "type": "STOCK_RESPONSE_BACKEND"
      },
      "methods": [
        "GET"
      ],
      "path": "/version"
    },
    {
      "backend": {
        "type": "HTTP_BACKEND",
        "url": "https://swapi.dev/api/people/${request.path[characterId]}"
      },
      "methods": [
        "GET"
      ],
      "path": "/characters/{characterId}"
    }
  ]
}